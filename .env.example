# Copy this file to `.env.local` and fill in your key.
# Do NOT commit `.env.local`.
EXA_API_KEY=

# Optional: override Exa API endpoint (default: https://api.exa.ai/search)
# Examples:
#   EXA_API_BASE_URL=https://exa.firstsound.me
#   EXA_SEARCH_ENDPOINT=https://exa.firstsound.me/search
EXA_API_BASE_URL=
EXA_SEARCH_ENDPOINT=

# Optional: auth header mode (default: x-api-key). Supported: x-api-key | bearer | both
EXA_AUTH_MODE=

# Optional: /api Exa fetch timeout in ms (default: 3000)
EXA_TIMEOUT_MS=
